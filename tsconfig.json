{
    "compilerOptions": {
      "target": "ES2017",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "outDir": "./dist",
      "baseUrl": "."
    },
    "include": ["./src", "./**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  